{
  "rewrites":[
    { "source":"/webhook/:path*",
      "destination":"https://n8n.sistemavieira.com.br/webhook/:path*" },
    { "source":"/api/:path*",
      "destination":"https://n8n.sistemavieira.com.br/api/:path*" },
    { "source": "/(.*)",
      "destination": "/" }
  ],
  "headers": [
    {
      "source": "/webhook/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}
